{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start -p $PORT",
    "type-check": "tsc",
    "remove_images": "docker rmi -f zapad-pet-frontend registry.heroku.com/zapad-pet-frontend/web",
    "build_image": "docker build . -t zapad-pet-frontend",
    "h:push": "heroku container:login && heroku container:push web --app zapad-pet-frontend",
    "h:release": "heroku container:release web --app zapad-pet-frontend",
    "h:build": "yarn remove_images && yarn build_image && yarn h:push && yarn h:release",
    "h:logs": "heroku logs --tail --app zapad-pet-frontend",
    "lint": "eslint \"{src,apps}/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"{src,apps}/**/*.{ts,tsx}\" --fix"
  },
  "dependencies": {
    "@svgr/webpack": "^5.5.0",
    "@types/classnames": "^2.2.11",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "@zeit/next-sass": "^1.0.1",
    "classnames": "^2.2.6",
    "config": "^3.3.3",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.7",
    "next": "latest",
    "node-sass": "^5.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-slidedown": "^2.4.5"
  },
  "devDependencies": {
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "license": "MIT"
}
